# BizOSaaS AI Crew System Environment Configuration

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=true

# Database Configuration
POSTGRES_PASSWORD=your_postgres_password
DATABASE_URL=postgresql://postgres:your_postgres_password@localhost:5432/bizosaas
REDIS_URL=redis://localhost:6379/0

# AI Service API Keys (Required for agent operations)
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key
GROQ_API_KEY=gsk_your-groq-api-key

# Optional AI Services
HUGGINGFACE_API_KEY=hf_your-huggingface-token
TOGETHER_AI_API_KEY=your-together-ai-key
REPLICATE_API_TOKEN=r8_your-replicate-token

# BizOSaaS Integration
BRAIN_API_URL=http://localhost:8001
CREW_SYSTEM_SECRET=crew-secret-key-change-in-production

# Performance Monitoring
MONITORING_ENABLED=true
METRICS_RETENTION_DAYS=7
ALERT_WEBHOOK_URL=https://your-webhook-url.com/alerts

# Crew System Configuration
DEFAULT_CREW_TIMEOUT=300
MAX_CONCURRENT_WORKFLOWS=10
ENABLE_CREW_CACHING=true
CREW_MEMORY_LIMIT=1000

# Agent Configuration
AGENT_MAX_ITERATIONS=10
AGENT_EXECUTION_TIMEOUT=120
ENABLE_AGENT_MEMORY=true

# Security
JWT_SECRET=your-jwt-secret-key
API_KEY_ENCRYPTION_SECRET=your-api-key-encryption-secret

# External Services (Optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/your-webhook
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your-webhook

# Development Settings
HOT_RELOAD=true
ENABLE_DOCS=true
CORS_ORIGINS=*

# Production Settings (uncomment for production)
# ENVIRONMENT=production
# DEBUG=false
# CORS_ORIGINS=https://your-domain.com,https://admin.your-domain.com
# LOG_LEVEL=WARNING

# Grafana (Optional)
GRAFANA_PASSWORD=admin

# Monitoring Endpoints
PROMETHEUS_METRICS_ENABLED=true
HEALTH_CHECK_INTERVAL=30