# BizOSaaS Admin Portal Configuration

# Server Configuration
PORT=3009
NODE_ENV=development

# Central Hub API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8001
NEXT_PUBLIC_BRAIN_API_URL=http://localhost:8001
NEXT_PUBLIC_AUTH_API_URL=http://localhost:8001

# Admin Configuration
NEXTAUTH_URL=http://localhost:3009
NEXTAUTH_SECRET=your-admin-nextauth-secret

# Multi-tenant Admin Configuration
NEXT_PUBLIC_APP_NAME="BizOSaaS Admin Portal"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXT_PUBLIC_ADMIN_LEVEL=super

# Feature Flags
NEXT_PUBLIC_ENABLE_TENANT_MANAGEMENT=true
NEXT_PUBLIC_ENABLE_USER_MANAGEMENT=true
NEXT_PUBLIC_ENABLE_SYSTEM_MONITORING=true
NEXT_PUBLIC_ENABLE_AI_AGENT_CONTROL=true
NEXT_PUBLIC_ENABLE_REVENUE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_SECURITY_AUDIT=true

# Admin Chat Configuration
NEXT_PUBLIC_ENABLE_ADMIN_CHAT=true
NEXT_PUBLIC_CHAT_API_ENDPOINT=/api/brain/admin/chat

# Development Settings
NEXT_PUBLIC_DEBUG=false
NEXT_TELEMETRY_DISABLED=1

# Security Configuration
NEXT_PUBLIC_SECURE_ADMIN_MODE=true
NEXT_PUBLIC_AUDIT_LOGGING=true