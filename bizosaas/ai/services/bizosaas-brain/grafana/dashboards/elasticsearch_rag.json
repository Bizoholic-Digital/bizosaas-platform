{
  "dashboard": {
    "title": "Elasticsearch RAG Performance",
    "tags": ["elasticsearch", "rag", "cohere"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "RAG Query Rate",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(rag_queries_total[5m])",
            "legendFormat": "Queries/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Queries/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Retrieval + Rerank Latency",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_retrieval_latency_seconds_bucket[5m]))",
            "legendFormat": "Retrieval (p95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(rag_rerank_latency_seconds_bucket[5m]))",
            "legendFormat": "Rerank (p95)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(rag_total_latency_seconds_bucket[5m]))",
            "legendFormat": "Total (p95)",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Average Relevance Score",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "avg(rag_avg_relevance_score)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2,
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.75, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Avg Documents Retrieved",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "avg(rag_documents_retrieved)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 1
          }
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Elasticsearch Cluster Health",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "elasticsearch_cluster_health_status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "options": {"0": {"text": "Red", "color": "red"}}},
              {"type": "value", "options": {"1": {"text": "Yellow", "color": "yellow"}}},
              {"type": "value", "options": {"2": {"text": "Green", "color": "green"}}}
            ]
          }
        }
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Total Indexed Documents",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "elasticsearch_indices_docs_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Elasticsearch Query Performance",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(elasticsearch_indices_search_query_total[5m])",
            "legendFormat": "{{index}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Queries/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "type": "graph",
        "title": "Cohere Rerank Performance",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(cohere_rerank_latency_seconds_bucket[5m]))",
            "legendFormat": "Latency (p95)",
            "refId": "A"
          },
          {
            "expr": "rate(cohere_rerank_requests_total[5m])",
            "legendFormat": "Request Rate",
            "refId": "B"
          }
        ]
      },
      {
        "id": 9,
        "type": "table",
        "title": "Top Queries by Frequency",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "topk(10, sum by (query) (increase(rag_queries_total[1h])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "type": "table",
        "title": "Slowest Queries",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "topk(10, max by (query) (rag_total_latency_seconds))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      },
      {
        "id": 11,
        "type": "bargauge",
        "title": "Index Size by Type",
        "gridPos": {"x": 0, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "elasticsearch_indices_store_size_bytes",
            "legendFormat": "{{index}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "decimals": 2
          }
        }
      },
      {
        "id": 12,
        "type": "graph",
        "title": "RAG Success Rate",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(rag_queries_total{status=\"success\"}[5m])) / sum(rate(rag_queries_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success Rate", "min": 0, "max": 100},
          {"format": "short"}
        ]
      }
    ]
  }
}
