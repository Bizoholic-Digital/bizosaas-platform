# BizOSaaS Marketing Strategist AI Environment Configuration

# Service Configuration
SERVICE_NAME=Marketing Strategist AI
SERVICE_PORT=8029
SERVICE_VERSION=1.0.0

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/bizosaas
POSTGRES_PASSWORD=postgres

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Brain API Integration
BRAIN_API_URL=http://localhost:8001

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this
ENCRYPTION_KEY=your-encryption-key-32-chars-long

# AI Service API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Marketing Platform API Keys
GOOGLE_ADS_API_KEY=your-google-ads-api-key
GOOGLE_ADS_CLIENT_ID=your-google-ads-client-id
GOOGLE_ADS_CLIENT_SECRET=your-google-ads-client-secret
GOOGLE_ADS_REFRESH_TOKEN=your-google-ads-refresh-token
GOOGLE_ADS_DEVELOPER_TOKEN=your-google-ads-developer-token

META_ADS_API_KEY=your-meta-ads-api-key
META_ADS_APP_ID=your-meta-ads-app-id
META_ADS_APP_SECRET=your-meta-ads-app-secret
META_ADS_ACCESS_TOKEN=your-meta-ads-access-token

LINKEDIN_ADS_API_KEY=your-linkedin-ads-api-key
LINKEDIN_ADS_CLIENT_ID=your-linkedin-ads-client-id
LINKEDIN_ADS_CLIENT_SECRET=your-linkedin-ads-client-secret
LINKEDIN_ADS_ACCESS_TOKEN=your-linkedin-ads-access-token

TIKTOK_ADS_API_KEY=your-tiktok-ads-api-key
TIKTOK_ADS_APP_ID=your-tiktok-ads-app-id
TIKTOK_ADS_SECRET=your-tiktok-ads-secret
TIKTOK_ADS_ACCESS_TOKEN=your-tiktok-ads-access-token

YOUTUBE_ADS_API_KEY=your-youtube-ads-api-key

# Email/SMTP Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_USE_TLS=true
SMTP_FROM_EMAIL=noreply@bizosaas.com
SMTP_FROM_NAME=BizOSaaS Marketing Team

# SMS Configuration (Optional)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number

# Analytics and Tracking
GOOGLE_ANALYTICS_ID=your-google-analytics-id
MIXPANEL_TOKEN=your-mixpanel-token

# External Service URLs
API_KEY_MANAGEMENT_URL=http://localhost:8026
PRODUCT_SOURCING_URL=http://localhost:8026
SUPPLIER_VALIDATION_URL=http://localhost:8027
ADMIN_AI_ASSISTANT_URL=http://localhost:8028

# Caching Configuration
CACHE_TTL_STRATEGIES=3600
CACHE_TTL_INSIGHTS=1800
CACHE_TTL_ANALYTICS=900

# AI Model Configuration
DEFAULT_AI_MODEL=gpt-4
BACKUP_AI_MODEL=claude-3-sonnet
MAX_TOKENS=4000
TEMPERATURE=0.7

# Performance Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090
LOG_LEVEL=INFO

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Security Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,https://app.bizosaas.com
CORS_CREDENTIALS=true
MAX_CONTENT_LENGTH=10485760

# Development Settings
DEBUG=false
TESTING=false
DEVELOPMENT=false

# Backup and Recovery
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30

# Feature Flags
ENABLE_COMPETITOR_ANALYSIS=true
ENABLE_BUDGET_OPTIMIZATION=true
ENABLE_CLIENT_COMMUNICATION=true
ENABLE_AUTOMATED_REPORTING=true
ENABLE_CONTENT_STRATEGY=true
ENABLE_PLATFORM_INTEGRATIONS=true